package com.TestApi.start.config;


import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.Configuration;
import org.springframework.http.HttpHeaders;
import org.springframework.stereotype.Component;

import java.util.List;


@Component
@ConfigurationProperties(prefix = "api")
@Configuration
public class ApiConfig {

    private List<String> urls;
    private Headers headers;


    public List<String> getUrls() {
        return urls;
    }

    public void setUrls(List<String> urls) {
        this.urls = urls;
    }

    public Headers getHeaders() {
        return headers;
    }

    public void setHeaders(Headers headers) {
        this.headers = headers;
    }



    public static class Headers{

        private String contentType;
        private String authorization;
        private String accept;
        private String acceptEncoding;
        private String connection;
        private String origin;
        private String referer;
        private String userAgent;
        private String tenantID;
        private String channelID;
        private String requestCountry;
        private String transactionReference;
        private String requesterID;
        private String requesterKey;
        private String inputTimestamp;
        private String language;
        private String dataSource;
        private String user;
        private String requestChannel;
        private String channelReference;
        private String identifierType;
        private String inputCountry;

        public String getChannelID() {
            return channelID;
        }

        public void setChannelID(String channelID) {
            this.channelID = channelID;
        }

        public String getRequestCountry() {
            return requestCountry;
        }

        public void setRequestCountry(String requestCountry) {
            this.requestCountry = requestCountry;
        }

        public String getTransactionReference() {
            return transactionReference;
        }

        public void setTransactionReference(String transactionReference) {
            this.transactionReference = transactionReference;
        }

        public String getRequesterID() {
            return requesterID;
        }

        public void setRequesterID(String requesterID) {
            this.requesterID = requesterID;
        }

        public String getRequesterKey() {
            return requesterKey;
        }

        public void setRequesterKey(String requesterKey) {
            this.requesterKey = requesterKey;
        }

        public String getInputTimestamp() {
            return inputTimestamp;
        }

        public void setInputTimestamp(String inputTimestamp) {
            this.inputTimestamp = inputTimestamp;
        }

        public String getLanguage() {
            return language;
        }

        public void setLanguage(String language) {
            this.language = language;
        }

        public String getDataSource() {
            return dataSource;
        }

        public void setDataSource(String dataSource) {
            this.dataSource = dataSource;
        }

        public String getUser() {
            return user;
        }

        public void setUser(String user) {
            this.user = user;
        }

        public String getRequestChannel() {
            return requestChannel;
        }

        public void setRequestChannel(String requestChannel) {
            this.requestChannel = requestChannel;
        }

        public String getChannelReference() {
            return channelReference;
        }

        public void setChannelReference(String channelReference) {
            this.channelReference = channelReference;
        }

        public String getIdentifierType() {
            return identifierType;
        }

        public void setIdentifierType(String identifierType) {
            this.identifierType = identifierType;
        }

        public String getInputCountry() {
            return inputCountry;
        }

        public void setInputCountry(String inputCountry) {
            this.inputCountry = inputCountry;
        }

        public String getConnection() {
            return connection;
        }

        public void setConnection(String connection) {
            this.connection = connection;
        }

        public String getOrigin() {
            return origin;
        }

        public void setOrigin(String origin) {
            this.origin = origin;
        }

        public String getReferer() {
            return referer;
        }

        public void setReferer(String referer) {
            this.referer = referer;
        }

        public String getUserAgent() {
            return userAgent;
        }

        public void setUserAgent(String userAgent) {
            this.userAgent = userAgent;
        }

        public String getTenantID() {
            return tenantID;
        }

        public void setTenantID(String tenantID) {
            this.tenantID = tenantID;
        }

        public String getAccept() {
            return accept;
        }

        public void setAccept(String accept) {
            this.accept = accept;
        }

        public String getAcceptEncoding() {
            return acceptEncoding;
        }

        public void setAcceptEncoding(String acceptEncoding) {
            this.acceptEncoding = acceptEncoding;
        }

        public String getAuthorization() {
            return authorization;
        }

        public void setAuthorization(String authorization) {
            this.authorization = authorization;
        }

        public String getContentType() {
            return contentType;
        }

        public void setContentType(String contentType) {
            this.contentType = contentType;
        }
    }

    @Bean
    public HttpHeaders commonHeaders(){

        HttpHeaders httpHeaders = new HttpHeaders();
        HttpHeaders headers = new HttpHeaders();
        headers.set("Content-Type", this.getHeaders().getContentType());
        headers.set("Accept", this.getHeaders().getAccept());
        headers.set("Accept-Encoding", this.getHeaders().getAcceptEncoding());
        headers.set("Connection", this.getHeaders().getConnection());
        headers.set("Origin", this.getHeaders().getOrigin());
        headers.set("Referer", this.getHeaders().getReferer());
        headers.set("User-Agent", this.getHeaders().getUserAgent());
        headers.set("tenantId", this.getHeaders().getTenantID());
        headers.set("channelId", this.getHeaders().getChannelID());
        headers.set("requestcountry", this.getHeaders().getRequestCountry());
        headers.set("transactionReference", this.getHeaders().getTransactionReference());
        headers.set("requesterId", this.getHeaders().getRequesterID());
        headers.set("requesterKey", this.getHeaders().getRequesterKey());
        headers.set("inputTimestamp", this.getHeaders().getInputTimestamp());
        headers.set("language", this.getHeaders().getLanguage());
        headers.set("dataSource", this.getHeaders().getDataSource());
        headers.set("user", this.getHeaders().getUser());
        headers.set("requestChannel", this.getHeaders().getRequestChannel());
        headers.set("channelReference", this.getHeaders().getChannelReference());

        headers.set("identifiertype", this.getHeaders().getIdentifierType());
        headers.set("inputcountry", this.getHeaders().getInputCountry());
        headers.setBearerAuth(this.getHeaders().getAuthorization());

        return headers;


    }
}
