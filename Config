	public boolean checkIfFlagsIsPrm(MainModel mainModel) {
		log.info("checkIfFlagsIsPrm()");
		NewPartyModel newPartyModel = mainModel.getNewPartyModel();
		NewPartyModelHeaders headers = mainModel.getHeaders();
		if (!ObjectUtils.isEmpty(newPartyModel) && !ObjectUtils.isEmpty(newPartyModel.getFlagsAndAttributes())
				&& !CollectionUtils.isEmpty(newPartyModel.getFlagsAndAttributes().getFlags())) {
			for (Flag flag : newPartyModel.getFlagsAndAttributes().getFlags()) {
				String code = checkTranslation(flag.get_identifier());
				if (!ObjectUtils.isEmpty(code) && code.equalsIgnoreCase(Others.PRM)) {
					String role = headers.getRole();
					Relationship relationship = role == null || role.isBlank()
							? goldenRelationshipRepository.getGlobalIdByRelatedProductAndRelationCountryAndAhRole(
							headers.getCustomerId(),
							headers.getInputCountry()
					) : goldenRelationshipRepository.getGlobalIdByRelatedProductAndRelationCountryAndRole(
							headers.getCustomerId(),
							headers.getInputCountry(),
							role
					);
					FlagsAttribute flagsAttribute = mapFlagsAttrbiuteToFlagsAndAttributes(flag, relationship);
					FlagsAttribute attribute = flagsAttributeRepository.getFlagsAttribute(flagsAttribute.getGlobalId(),
							HelperUtils.checkTranslation(flagsAttribute.getRole()), flagsAttribute.getRelationCountry(),
							flagsAttribute.getRelationshipSequence(), flagsAttribute.getOrganization(),
							flagsAttribute.getValue(), HelperUtils.checkTranslation(flagsAttribute.getType()));
					if (!ObjectUtils.isEmpty(attribute))
						flagsAttributeRepository.delete(attribute);
					flagsAttributeRepository.save(flagsAttribute);
					return true;
				}
			}
		}
		log.info("/checkIfFlagsIsPrm()");
		return false;
	}
