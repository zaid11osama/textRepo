package com.TestApi.start.config;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.stereotype.Component;

import java.util.List;
import java.util.Map;

@Component
@ConfigurationProperties(prefix = "api2")
public class Curl {

    private Curls curls;
    private Map<String, String> headers;

    public Curls getCurls() {
        return curls;
    }

    public void setCurls(Curls curls) {
        this.curls = curls;
    }

    public Map<String, String> getHeaders() {
        return headers;
    }

    public void setHeaders(Map<String, String> headers) {
        this.headers = headers;
    }

    public static class Curls {
        private List<String> get;
        private List<Post> post;
        private List<Put> put;
        private List<Delete> delete;

        public List<String> getGet() {
            return get;
        }

        public void setGet(List<String> get) {
            this.get = get;
        }

        public List<Post> getPost() {
            return post;
        }

        public void setPost(List<Post> post) {
            this.post = post;
        }

        public List<Put> getPut() {
            return put;
        }

        public void setPut(List<Put> put) {
            this.put = put;
        }

        public List<Delete> getDelete() {
            return delete;
        }

        public void setDelete(List<Delete> delete) {
            this.delete = delete;
        }
    }

    public static class Post {
        private String url;
        private String body;

        public String getUrl() {
            return url;
        }

        public void setUrl(String url) {
            this.url = url;
        }

        public String getBody() {
            return body;
        }

        public void setBody(String body) {
            this.body = body;
        }
    }

    public static class Put {
        private String url;
        private String body;

        public String getUrl() {
            return url;
        }

        public void setUrl(String url) {
            this.url = url;
        }

        public String getBody() {
            return body;
        }

        public void setBody(String body) {
            this.body = body;
        }
    }

    public static class Delete {
        private String url;
        private String body;

        public String getUrl() {
            return url;
        }

        public void setUrl(String url) {
            this.url = url;
        }

        public String getBody() {
            return body;
        }

        public void setBody(String body) {
            this.body = body;
        }
    }
}





