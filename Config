	 String  handelRiskRating(NewPartyModel newPartyModel, Map<String, String> headers) throws Exception {
		if (newPartyModel.getComplianceDetails() != null && newPartyModel.getComplianceDetails().get_riskRating() != null
				&& newPartyModel.getComplianceDetails().get_riskRating().getCode() != null
				&& newPartyModel.getComplianceDetails().get_riskRating().getCode().equalsIgnoreCase(Constants.RR.CALC_RR_FLAG)) {
			newPartyModel.getComplianceDetails().get_riskRating().setCode(soapRequestService.getRiskRating(newPartyModel, headers));
			return soapRequestService.getRiskRating(newPartyModel, headers);
		}
		return null;
	}
