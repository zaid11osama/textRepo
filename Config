public static PartyComplianceDetails mapToPartyComplianceDetails(
			com.arabbank.model.entity.golden.ComplianceDetails complianceDetails, ComplianceBasic complianceBasic) {
		log.info("mapToPartyComplianceDetails()");
		PartyComplianceDetails partyComplianceDetails = new PartyComplianceDetails();
		if (!ObjectUtils.isEmpty(complianceDetails)) {
			Optional.of(complianceDetails).ifPresent(details -> {
				partyComplianceDetails.set_riskRating(details.getRiskRating());
				partyComplianceDetails.setLastReviewDate(details.getLastReviewDate());
				partyComplianceDetails.setNextReviewDate(details.getNextReviewDate());
				partyComplianceDetails.setRatingFrequency(details.getRatingFrequency());
				partyComplianceDetails.setGrcFlag(details.getGrcFlag() != null && details.getGrcFlag() == 1);
				partyComplianceDetails.set_dataStatus(details.getDataStatus());
				partyComplianceDetails.setGrcModificationDate(details.getGrcModifyDate());
			});
		}

		if (!ObjectUtils.isEmpty(complianceBasic)) {
			Optional.of(complianceBasic).ifPresent(details -> {
				partyComplianceDetails.set_residencyCountry(details.getResidencyCountry());
				partyComplianceDetails.set_residencyIndicator(details.getResidencyIndicator());
			});
		}
		log.info("/mapToPartyComplianceDetails()");
		return partyComplianceDetails;
	}
