   @Bean
    public HttpHeaders commonHeaders() {
        HttpHeaders headers = new HttpHeaders();
        headers.set("Content-Type", apiConfig.getHeaders().getContentType());
        headers.set("Accept", apiConfig.getHeaders().getAccept());
        headers.set("Accept-Encoding", apiConfig.getHeaders().getAcceptEncoding());
        headers.set("Connection", apiConfig.getHeaders().getConnection());
        headers.set("Origin", apiConfig.getHeaders().getOrigin());
        headers.set("Referer", apiConfig.getHeaders().getReferer());
        headers.set("User-Agent", apiConfig.getHeaders().getUserAgent());
        headers.set("tenantId", apiConfig.getHeaders().getTenantID());
        headers.set("channelId", apiConfig.getHeaders().getChannelID());
        headers.set("requestcountry", apiConfig.getHeaders().getRequestCountry());
        headers.set("transactionReference", apiConfig.getHeaders().getTransactionReference());
        headers.set("requesterId", apiConfig.getHeaders().getRequesterID());
        headers.set("requesterKey", apiConfig.getHeaders().getRequesterKey());
        headers.set("inputTimestamp", apiConfig.getHeaders().getInputTimestamp());
        headers.set("language", apiConfig.getHeaders().getLanguage());
        headers.set("dataSource", apiConfig.getHeaders().getDataSource());
        headers.set("user", apiConfig.getHeaders().getUser());
        headers.set("requestChannel", apiConfig.getHeaders().getRequestChannel());
        headers.set("channelReference", apiConfig.getHeaders().getChannelReference());
        headers.set("identifiertype", apiConfig.getHeaders().getIdentifierType());
        headers.set("inputcountry", apiConfig.getHeaders().getInputCountry());
        headers.setBearerAuth(apiConfig.getHeaders().getAuthorization());

        return headers;
    }
