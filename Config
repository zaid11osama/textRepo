	private static DocumentDetail setDocumntsMapping(com.arabbank.model.entity.golden.Document document) {
		DocumentDetail documentDetail = new DocumentDetail();
		PartyArchiveDetails archiveDetails = new PartyArchiveDetails();
		documentDetail.set_country(document.getDocumentCountry());
		documentDetail.set_issueCountry(document.getIssueCountry());
		documentDetail.setExpiryDate(document.getExpiryDate());
		documentDetail.setIssueDate(document.getIssueDate());
		documentDetail.setIssuedBy(document.getIssuedBy());
		documentDetail.setIssuePlace(document.getIssuePlace());
		documentDetail.setNumber(document.getDocumentNumber());
		documentDetail.setUsage(document.getUsage());
		archiveDetails.setDmsReference(document.getDmsId());
		archiveDetails.setArchivalStatus(
				document.getCrmStatus() != null && (document.getCrmStatus().equalsIgnoreCase("Success")));
		documentDetail.setArchiveDetails(archiveDetails);
		return documentDetail;
	}
