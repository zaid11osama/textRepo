public void cacheTranslations(String fileKey) {
		log.debug("casheTranslations()");
		fileKey = fileKey == null ? " " : fileKey;
		List<MdmSysTranslations> list = null;
		list = findByFileKey(fileKey.toUpperCase());
		if (list != null) {
			MdmSysTranslations lookUps = new MdmSysTranslations();
			translationsMap.put(fileKey, lookUps);
		}
		for (MdmSysTranslations looUps : list) {
			String[] filtersKeys = checkTheFilterationKey3(looUps);
			for (String filterKey3 : filtersKeys) {
				String key = concatTranslation(fileKey, filterKey3, looUps == null ? null : looUps.getLanguage(),
						looUps == null ? null : looUps.getCode());
				translationsMap.put(key, looUps);
			}
		}
		log.debug("/casheTranslations()");
	}
