public Object getSpecificModel(String modelName, String identifier, Map<String, String> headers) throws Exception {
		log.info("getSpecificModel()");
		String corporationId = generateReference();
		String endpointName = EndpointsName.GET_SPECIFIC_AB + "/" + identifier + "/" + modelName;
		LogChannelEntity logChannel = dbLogger.logIntoDb(identifier, headers, corporationId, endpointName);


		try {
			Object response = partyService.generalGet(headers, identifier, modelName);
			dbLogger.updateLogChannelOutput(objectMapper.writeValueAsString(response), HttpStatus.OK.name(),
					HttpStatus.OK.value(), logChannel);
			return response;
		} catch (Exception e) {
			log.error(e.getMessage());
			dbLogger.updateLogChannelOutput(e.getMessage(), HttpStatus.BAD_REQUEST.name(),
					HttpStatus.BAD_REQUEST.value(), logChannel);
			throw e;
		} finally {
			log.info("/getSpecificModel()");
		}
	}
