	public void checkResponse(ResponseEntity<ResponseModel> response) throws Exception {
		if (!ObjectUtils.isEmpty(response) && response.getBody().getStatus().equalsIgnoreCase("S"))
			return;
		else if (response.getBody().getStatus().equalsIgnoreCase("E"))
			throw new BadRequestException(response.getBody().getDetails());
		else
			throw new Exception(response.getBody().getDetails());
	}
