public String getGlobalId(String prefix) {
		log.info("getGlobalId({}})", prefix);
		String url = getFeshehUtilityBaseUrl() + "/mdm/v1/util/global-id/" + prefix;
		ResponseEntity<String> response = restTemplate.getForEntity(url, String.class);
		log.info("generated GlobalId : {}", response.getBody().toString());
		log.info("/getGlobalId({})", prefix);
		return response.getBody().toString();
	}
