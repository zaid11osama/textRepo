public class LogsChannelMapper implements RowMapper<LogChannel> {

	@Override
	public LogChannel mapRow(ResultSet rs, int rowNum) throws SQLException {
		LogChannel logChannel = new LogChannel();
		logChannel.setChannelReference(rs.getString("CHANNEL_REFERENCE"));
		logChannel.setResponseMessages(rs.getString("RESPONSE_MESSAGES"));
		logChannel.setData(rs.getString("DATA"));
		return logChannel;
	}

}
