public static Fatca mapFatcaDetailsToFatca(com.arabbank.model.entity.golden.FatcaDetails fatcaDetails) {
		log.info("mapFatcaDetailsToFatca()");
		Fatca fatca = new Fatca();
		Questions questions = new Questions();
		GrcDetails grcDetails = new GrcDetails();
		if (!ObjectUtils.isEmpty(fatcaDetails)) {
			Optional.ofNullable(fatcaDetails).ifPresent(details -> {
				questions.setIsUSCitizen(details.getIsUsCitizen());
				questions.setIsUSBorn(details.getIsUsBorn());
				questions.setIsGreencardHolder(details.getIsGreenCardHolder());
				questions.setIsUSResident(details.getIsUsResident());
				questions.setIsUSTaxPayer(details.getIsUsTaxPayer());
				questions.setSignedW8(details.getSignedW8());
				questions.setSignedW9(details.getSignedW9());
				questions.setSignedSecrecyWaiver(details.getSignedSecrecyWaiver());
				questions.setHasUSEntityOwner(details.getHaUsEntityOwner());
				fatca.setQuestions(questions);
				fatca.setTaxIdentificationNumber(details.getTin());
				fatca.setSignDate(details.getSignedDate());
				fatca.setDateFillingForm(details.getDateFillingForm());
				fatca.setLatestAmendment(details.getLatestAmendmentDate());
				grcDetails.set_fatcaClassification(details.getFatcaClassification());
				grcDetails.set_oldFatcaClassification(details.getPreviousfatcaClassification());
				grcDetails.setChangedByGRC("Y".equals(details.getIsChangedByGrc()));
				grcDetails.setBoException(details.getBoExcemption());
				grcDetails.set_excemptCode(new Translation().withCode(details.getExcemptCode()));
				grcDetails.set_excemptionReportingCode(new Translation().withCode(details.getExceptionReportingCode()));
				fatca.setGrcDetails(grcDetails);
			});
		}
		log.info("/mapFatcaDetailsToFatca()");
		return fatca;
	}
