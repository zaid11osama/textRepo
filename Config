    @Test
    void mapFlagsAttrbiuteToFlagsAndAttributes_ShouldReturnMappedObject() {
        // Arrange
        Flag flag = new Flag();
        flag.set_identifier(new Translation("FlagName", "PRM", "Preferred"));
        flag.setStatus("1");
        flag.setStatusDate(Date.valueOf(LocalDate.of(2025, 10, 5)));
        flag.setTermsAndConditionVersion("v1");
        flag.setValue("Active");

        Relationship relationship = new Relationship();
        relationship.setGlobalId("G123");
        relationship.setOrganization("Org001");
        relationship.setRelationCountry(new Translation("Country", "JO", "Jordan"));
        relationship.setRelationshipSequence("SEQ001");
        relationship.setRole("Admin");
        relationship.setLatestUniqueRefrence("REF001");

        // Act
        FlagsAttribute result = partyService.mapFlagsAttrbiuteToFlagsAndAttributes(flag, relationship);

        // Assert
        assertNotNull(result);
        assertEquals("G123", result.getGlobalId());
        assertEquals("Org001", result.getOrganization());
        assertEquals("SEQ001", result.getRelationshipSequence());
        assertEquals("Admin", result.getRole());
        assertEquals("Active", result.getValue());
        assertEquals("PRM", result.getType().getCode());
        assertEquals(1, result.getIsActive());
        assertEquals(0, result.getIsDeleted());
        assertEquals(Others.KYC, result.getAuthorizedUser());
        assertEquals(Others.KYC, result.getUpdateUser());
        assertNotNull(result.getUniversalId());
        assertNotNull(result.getUpdateTimestamp());
        assertEquals(flag.getStatusDate(), result.getActiveDate()); // since status = "1"
    }
