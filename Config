public String updateAbCustomer(NewPartyModel newPartyModel, Map<String, String> headers, String partyIdentifire)
			throws Exception {
		log.info("updateAbCustomer()");
		String corporationId = generateReference();
		LogChannelEntity logChannel = dbLogger.logIntoDb(newPartyModel, headers, corporationId,
				EndpointsName.UPDATE_AB);



		try {
			String response = partyService.updateAbTransaction(newPartyModel, headers, corporationId, partyIdentifire);
			dbLogger.updateLogChannelOutput(objectMapper.writeValueAsString(response), HttpStatus.OK.name(),
					HttpStatus.OK.value(), logChannel);
			return response;
		} catch (Exception e) {
			log.error(e.getMessage());
			dbLogger.updateLogChannelOutput(e.getMessage(), HttpStatus.BAD_REQUEST.name(),
					HttpStatus.BAD_REQUEST.value(), logChannel);
			throw e;
		} finally {
			log.info("/updateAbCustomer()");
		}
	}
