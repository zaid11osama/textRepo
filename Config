public class TranslationAttributesMapper implements RowMapper<TranslationsAttribute> {

	@Override
	public TranslationsAttribute mapRow(ResultSet rs, int rowNum) throws SQLException {
		TranslationsAttribute translationsAttr = new TranslationsAttribute();
		translationsAttr.setFileKey(rs.getString("FILE_KEY"));
		translationsAttr.setLanguage(rs.getString("LANGUAGE"));
		translationsAttr.setFiltrationKey1(rs.getString("FILTRATION_KEY1"));
		translationsAttr.setFiltrationKey2(rs.getString("FILTRATION_KEY2"));
		translationsAttr.setFiltrationKey3(rs.getString("FILTRATION_KEY3"));
		translationsAttr.setFiltrationKey4(rs.getString("FILTRATION_KEY4"));
		translationsAttr.setFiltrationKey5(rs.getString("FILTRATION_KEY5"));
		translationsAttr.setCode(rs.getString("CODE"));
		return translationsAttr;
	}
