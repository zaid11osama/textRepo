	public String[] checkTheFilterationKey3(MdmSysTranslations lookUps) {
		log.debug("checkTheFilterationKey3({})", lookUps);
		try {
			String[] filtersKeys;
			if (lookUps != null && lookUps.getFiltrantionKey3() != null && lookUps.getFiltrantionKey3().contains(",")) {
				filtersKeys = lookUps.getFiltrantionKey3().split(",\\s*");
			} else {
				filtersKeys = new String[] { lookUps != null ? lookUps.getFiltrantionKey3() : null };
			}
			return filtersKeys;
		} finally {
			log.debug("/checkTheFilterationKey3({})", lookUps);
		}
	}
