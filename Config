	public PartyModel getFullPartyDetails(Relationship relationship, Map<String, String> headers,
			String generatedKycReference) throws JsonProcessingException {
		log.info("getFullPartyDetails()");
		PartyModel partyModel = new PartyModel();
		RequestHeader requestHeaders = populateRequestHeader(headers);
		partyModel.setIdentifier(getPartyIdentifiers(relationship, requestHeaders.getLanguage()));
		partyModelSetter.setPartyModel(partyModel, relationship, headers);
		log.info("/getFullPartyDetails()");
		return partyModel;
	}
