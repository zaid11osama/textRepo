	public void setPhones(int index, Contact contact, PartyPhone partyPhone, String type) {
		PartyPhoneCountry country = new PartyPhoneCountry();
		partyPhone.setType(type);
		partyPhone.setNumber(contact.getPhone().getNumber());
		if (contact.getPhone().getNumber() != null)
			country.setDialcode(getDialCode(
					contact.getPhone().getCountry() != null ? contact.getPhone().getCountry().getCode() : null));
		country.setCode(HelperUtils.checkTranslation(contact.getPhone().getCountry()));
		country.setDescription(HelperUtils.checkDescription(contact.getPhone().getCountry()));
		partyPhone.setCountry(country);
		partyPhone.setIndex(index);
	}
