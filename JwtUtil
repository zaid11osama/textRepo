public class SoapRequestService {

	private final ConfigReader configReader;

	public String getRiskRating(NewPartyModel newPartyModel, Map<String, String> headers) throws Exception {
		CheckCustomerDueDiligenceRulesRequest request = fillRequest(newPartyModel, headers);
		SOAPMessage createSoapRequest = createSoapRequest(request);
		RiskRating riskRating = sendSoapRequest(createSoapRequest);
		return riskRating == null ? null : riskRating.getRiskRating();
	}
