 @Test
    void testCreateAbCustomer_shouldReturnCreatedResponse() throws Exception {
        // Arrange
        NewPartyModel newPartyModel = new NewPartyModel();
        Map<String, String> headers = new HashMap<>();
        headers.put("Authorization", "Bearer token");
        String identifier = "AB123";

        Object expectedResponse = new Object(); // you can use a custom object if you want

        when(mainService.createAbCustomer(eq(newPartyModel), eq(headers), eq(identifier)))
                .thenReturn(expectedResponse);

        // Act
        ResponseEntity<Object> response = partyController.createAbCustomer(newPartyModel, headers, identifier);

        // Assert
        assertNotNull(response);
        assertEquals(HttpStatus.CREATED, response.getStatusCode());
        assertEquals(expectedResponse, response.getBody());

        verify(mainService, times(1))
                .createAbCustomer(eq(newPartyModel), eq(headers), eq(identifier));
    }
