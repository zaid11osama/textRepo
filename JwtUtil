  @Test
    void shouldMapDtoToModelSuccessfully() throws JsonProcessingException {
        // given
        PartyModel dto = new PartyModel();
        Relationship rel = new Relationship();
        dto.setIdentifier(new Identifier("P123"));
        dto.setNames(List.of(new NameModel("John", "Doe")));

        // when
        NewPartyModel result = partyMapper.mapDtoToModel(dto, rel, "en", "JO");

        // then
        assertNotNull(result);
        assertEquals("P123", result.getIdentifier().getPartyId());
        assertFalse(result.getNames().isEmpty());
    }
