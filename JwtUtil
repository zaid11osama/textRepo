public class LogMainDto {

	private HeaderDto header;
	private String inputTimestamp;
	private String outputTimestamp;
	private String channel;
	private String channelReference;
	private String mdmReference;
	private String totalSubs;
	private String status;
	private String message;
	private String numberOfRetries;
	private FooterDto footer;


}




public class OrchService {

	@Autowired
	private OrchMainRepository orchMainRepository;

	@Autowired
	private OrchSubMainRepository orchSubMainRepository;

	@Autowired
	private LogCupTransactionRepository logCupTransactionRepository;

	@Autowired
	private RelationshipRepository relationshipRepository;

	@Autowired
	private LockedCustomerRepository lockedCustomerRepository;

	public void insertLogMain(LogMainDto logMainDto) {
		try {
			log.info("insertLogMain({})", logMainDto);
			orchMainRepository.insertOrchMain(logMainDto.getHeader(), logMainDto.getFooter());
		} catch (RuntimeException ex) {
			log.error(ex.getMessage());
			throw new GoldenMDMException("Error inserting log for main");
		} finally {
			log.info("/insertLogMain({})", logMainDto);
		}
	}
