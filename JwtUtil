java.lang.NullPointerException: Cannot invoke "java.util.List.stream()" because the return value of "com.arabbank.model.partymodel.NewPartyModel.getNationalities()" is null



	private void checkAndThrowIfPartyHasIdentificationNumber(NewPartyModel newPartyModel,ABCustomerRequestHeader abCustomerRequestHeader) {
		newPartyModel.getNationalities().stream()
				.filter(this::isIdentificationNumberFilled)
				.forEach(nationality -> {
					throwIfCustomerAlreadyExists(newPartyModel, abCustomerRequestHeader.getRequestCountry());
					throwIfCustomerIsBeingProcessed(newPartyModel);
				});
	}

@JsonFormat(shape = JsonFormat.Shape.STRING, pattern = "yyyy-MM-dd", timezone = "JST")
@JsonPropertyOrder({ "identifier" })
@Data
@JsonInclude(JsonInclude.Include.NON_NULL)
public class NewPartyModel {
	@JsonProperty(value = "identifiers")
	private PartyIdentifier identifier;
	@JsonProperty(value = "personalDetails")
	private PersonalDetails personalDetails;
	@JsonProperty(value = "basicDetails")
	private BasicDetails basicDetails;
	@JsonProperty(value = "names")
	private Map<String, PartyName> names;
	@JsonProperty(value = "addresses")
	private Map<String, PartyAddress> addresses;
	@JsonProperty(value = "contacts")
	private PartyContact contacts;
	@JsonProperty(value = "nationalities")
	private List<PartyNationality> nationalities;
	@JsonProperty(value = "documents")
	private PartyDocument documents;
	@JsonProperty(value = "employment")
	private Employment employment;
	@JsonProperty(value = "portfolio")
	private PartyPortfolio portfolio;
	@JsonProperty(value = "flagsAndAttributes")
	private FlagsAndAttributes flagsAndAttributes;
	@JsonProperty(value = "fatca")
	private Fatca fatca;
	@JsonProperty(value = "complianceDetails")
	private PartyComplianceDetails complianceDetails;
	@JsonProperty(value = "images")
	private List<PartyImage> images;
	private Map<String, Object> codeDescriptions;
	@JsonProperty(value = "dueDiligence")
	private PartyDueDiligence dueDiligence;
	@JsonProperty(value = "taxDetails")
	private PartyTaxDetails taxDetails;
	@JsonProperty(value = "pepDetails")
	private PepDetails pepDetails;
	@JsonProperty(value = "socialDetails")
	private SocialDetails socialDetails;
	@JsonProperty(value = "otherBanks")
	private List<OtherBanks> otherBanks;
	@JsonProperty(value = "reopenCustomer")
	private boolean reopenCustomer;
}
