    @Test
    void checkHeaderNulls_ShouldPass_WhenAllHeadersExist() {
        // Arrange
        Map<String, String> headers = new HashMap<>();
        headers.put(MdmHeaderRequest.USER, "testUser");
        headers.put(MdmHeaderRequest.INPUT_COUNTRY, "AE");
        headers.put(MdmHeaderRequest.REQUEST_CHANNEL, "WEB");
        headers.put(MdmHeaderRequest.REQUEST_LANGUAGE, "EN");
        headers.put(MdmHeaderRequest.REQUEST_REFERENCE, "REF123");

        // Act & Assert â†’ should not throw
        assertDoesNotThrow(() -> partyService.checkHeaderNulls(headers));
    }
