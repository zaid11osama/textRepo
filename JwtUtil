 @Test
    void testCheckTheFilterationKey3_WithCommaSeparatedKeys() {
        // Arrange
        MdmSysTranslations lookup = new MdmSysTranslations();
        lookup.setFiltrantionKey3("A,B,C");

        // Act
        String[] result = translationsService.checkTheFilterationKey3(lookup);

        // Assert
        assertArrayEquals(new String[]{"A", "B", "C"}, result);
    }

    @Test
    void testCheckTheFilterationKey3_WithSpacesAfterComma() {
        // Arrange
        MdmSysTranslations lookup = new MdmSysTranslations();
        lookup.setFiltrantionKey3("A, B, C");

        // Act
        String[] result = translationsService.checkTheFilterationKey3(lookup);

        // Assert
        assertArrayEquals(new String[]{"A", "B", "C"}, result);
    }

    @Test
    void testCheckTheFilterationKey3_WithSingleValue() {
        // Arrange
        MdmSysTranslations lookup = new MdmSysTranslations();
        lookup.setFiltrantionKey3("XYZ");

        // Act
        String[] result = translationsService.checkTheFilterationKey3(lookup);

        // Assert
        assertArrayEquals(new String[]{"XYZ"}, result);
    }

    @Test
    void testCheckTheFilterationKey3_WithNullObject() {
        // Act
        String[] result = translationsService.checkTheFilterationKey3(null);

        // Assert
        assertArrayEquals(new String[]{null}, result);
    }

    @Test
    void testCheckTheFilterationKey3_WithNullFieldValue() {
        // Arrange
        MdmSysTranslations lookup = new MdmSysTranslations();
        lookup.setFiltrantionKey3(null);

        // Act
        String[] result = translationsService.checkTheFilterationKey3(lookup);

        // Assert
        assertArrayEquals(new String[]{null}, result);
    }
