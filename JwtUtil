@Test
    void shouldReturnTrueWhenAllFieldsAreEmpty() {
        PartyModel emptyParty = new PartyModel();
        boolean result = partyUtils.checkPartyModelData(emptyParty);
        assertTrue(result, "Expected true when all fields are null or empty");
    }

    @Test
    void shouldReturnFalseWhenOneFieldIsNotEmpty() {
        PartyModel party = new PartyModel();
        party.setNames(List.of(new NameModel())); // one field not empty

        boolean result = partyUtils.checkPartyModelData(party);
        assertFalse(result, "Expected false when at least one field is not empty");
    }

    @Test
    void shouldReturnFalseWhenComplianceDetailIsNotNull() {
        PartyModel party = new PartyModel();
        party.setComplianceDetail(new ComplianceDetail());

        boolean result = partyUtils.checkPartyModelData(party);
        assertFalse(result, "Expected false when ComplianceDetail is not null");
    }
