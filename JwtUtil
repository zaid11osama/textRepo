public class RequesterRawMapper implements RowMapper<Requester> {

	@Override
	public Requester mapRow(ResultSet rs, int rowNum) throws SQLException {
		Requester requester = new Requester();
		requester.setId(rs.getInt("ID"));
		requester.setRequesterId(rs.getString("REQUESTER_ID"));
		requester.setRequesterKey(rs.getString("REQUESTER_KEY"));
		requester.setRequesterStatus(rs.getInt("REQUESTER_STATUS"));
		requester.setDateRecordCreated(rs.getDate("DATE_RECORD_CREATED"));
		requester.setDateLastUpdated(rs.getDate("DATE_LAST_UPDATED"));
		requester.setUpdatedToday(rs.getInt("UPDATED_TODAY"));
		requester.setUpdateProgram(rs.getString("UPDATE_PROGRAM"));
		requester.setUpdateCountry(rs.getString("UPDATE_COUNTRY"));
		requester.setIsDeleted(rs.getInt("IS_DELETED"));
		requester.setDateDeleted(rs.getDate("DATE_DELETED"));
		requester.setUpdateTimestamp(rs.getTimestamp("UPDATE_TIMESTAMP"));
		return requester;
	}
	
