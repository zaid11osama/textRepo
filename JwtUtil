@Test
    void testMapRow_Success() throws SQLException {
        // Arrange
        ResultSet rs = mock(ResultSet.class);
        when(rs.getString("CHANNEL_REFERENCE")).thenReturn("REF123");
        when(rs.getString("RESPONSE_MESSAGES")).thenReturn("Message content");
        when(rs.getString("DATA")).thenReturn("Some data");

        LogsChannelMapper mapper = new LogsChannelMapper();

        // Act
        LogChannel result = mapper.mapRow(rs, 1);

        // Assert
        assertNotNull(result);
        assertEquals("REF123", result.getChannelReference());
        assertEquals("Message content", result.getResponseMessages());
        assertEquals("Some data", result.getData());
    }

    @Test
    void testMapRow_SQLExceptionThrown() throws SQLException {
        // Arrange
        ResultSet rs = mock(ResultSet.class);
        when(rs.getString(anyString())).thenThrow(new SQLException("Database error"));

        LogsChannelMapper mapper = new LogsChannelMapper();

        // Act & Assert
        SQLException exception = assertThrows(SQLException.class, () -> mapper.mapRow(rs, 1));
        assertEquals("Database error", exception.getMessage());
    }
