	public List<RelationshipModel> getRelationships(String globalId) {
		try {
			log.info("getRelationships(String globalId)");
			return relationshipRepository.findAllByGlobalIdAndIsDeletedFalse(globalId, globalId);
		} catch (RuntimeException ex) {
			log.error(ex.getMessage());
			throw new GoldenMDMException("error in fetching relationships");
		} finally {
			log.info("/getRelationships(String globalId)");
		}
	}
