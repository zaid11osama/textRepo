	public void produceDocuments(List<Document> documents, String generateCustomerId, String globalId,
			String nationalNumber, Map<String, String> headers, String customerName, String corporationId)
			throws JsonProcessingException {
		log.info("produceDocuments()");
		String inputCountry = headers.get(MdmHeaderRequest.INPUT_COUNTRY);
		String tenantId = headers.get(MdmHeaderRequest.TENANT_ID);
		for (Document document : documents) {
			String dmsId = document.getDmsId();
			String docType = HelperUtils.checkTranslation(document.getType());
			String documentNumber = document.getDocumentNumber();
			if (!isEmpty(dmsId) && !isEmpty(docType)) {
				saveDocument(docType, dmsId, globalId, corporationId, inputCountry,
						HelperUtils.checkTranslation(document.getDocumentCountry()), "-", documentNumber);
				kafkaUtil.produceMessage(generateCustomerId, globalId, nationalNumber, inputCountry, tenantId, dmsId,
						docType, documentNumber, customerName);
			}
		}
		log.info("/produceDocuments()");
	}
